@model IEnumerable<Member_Ship.Entities.Subscription>
@using Member_Ship.Areas.Admin.Models

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.Partial("_CreateButtonPartian")
</p>
<table class="table table-condensed table-striped" >
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegistrationCode)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegistrationCode)
            </td>
            <td>
                @Html.Partial("_tableButtonPartialView", new SmallButtonModel { Id = item.Id })
            </td>
        </tr>
    }

</table>
