@model IEnumerable<Member_Ship.Areas.Admin.Models.ProductItemModel>
@using Member_Ship.Extensions
@using Member_Ship.Areas.Admin.Models

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.Partial("_CreateButtonPartian")
</p>
<table class="table table-striped table-condensed">
    <tr class="success">
        <th>@Html.DisplayNameFor(Model => Model.ProductTitle)</th>
        <th>@Html.DisplayNameFor(Model => Model.ItemTitle)</th>
        <th></th>
    </tr>
  
    @foreach (var item in Model)
    {
        
           <tr>
            <td>@Html.DisplayFor(m => item.ProductTitle)</td>
            <td>@Html.DisplayFor(m => item.ItemTitle)</td>

            <td>
                @Html.Partial("_tableButtonPartialView", new SmallButtonModel
                     { ItemId=item.ItemId,ProductId=item.ProductId })

            </td>
            </tr>
        
    }
       

</table>
